<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.likunlin.firebase7">

    <uses-permission android:name="android.permission.USE_FINGERPRINT" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".save" />
        <activity android:name=".read" />
        <activity android:name=".togglebuttom" />
        <activity android:name=".fingerprint" />
        <activity android:name=".fingerprint2" />
        <activity android:name=".CircleProgressBar" />
        <activity android:name=".anim" />
        <activity android:name=".passwordeditview" />
        <activity android:name=".webview">
        </activity>
    </application>

</manifest>